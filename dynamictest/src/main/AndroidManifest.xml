<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-feature android:name="programmersbox.extension" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/baseline_catching_pokemon_24"
        android:label="@string/app_name">

        <meta-data
            android:name="programmersbox.extension.class"
            android:value="${extClass}" />
        <meta-data
            android:name="programmersbox.extension.name"
            android:value="${extName}" />
    </application>

</manifest>